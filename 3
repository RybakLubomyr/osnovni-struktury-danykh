#include <iostream>
using namespace std;
int main() {
  int arr[12][2];
  arr[0][0] = 2;
  arr[0][1] = 2;
  arr[1][0] = 4;
  arr[1][1] = 4;
  arr[2][0] = 7;
  arr[2][1] = 7;
  arr[3][0] = 2;
  arr[3][1] = 4;
  arr[4][0] = 3;
  arr[4][1] = 7;
  arr[5][0] = 4;
  arr[5][1] = 2;
  arr[6][0] = 6;
  arr[6][1] = 6;
  arr[7][0] = 3;
  arr[7][1] = 3;
  arr[8][0] = 15;
  arr[8][1] = 4;
  arr[9][0] = 15;
  arr[9][1] = 15;
  arr[10][0] = 7;
  arr[10][1] = 2;
  arr[11][0] = 2;
  arr[11][1] = 7;

  int point = 0;
  int pointMax = 0;
  int x1,x2,x3,x4,y1,y2,y3,y4;
  int dx1,dy1,dx,dy;
  int pointArr[4][2];
  int limit;

  for(int i = 0; i < 12; i++){
    for(int j = 1; j < 12; j++){
      for(int k = 2; k < 12; k++){
        for(int h = 3; h < 12; h++){
          x1 = arr[i][0];
          x2 = arr[j][0];
          x3 = arr[k][0];
          x4 = arr[h][0];

          y1 = arr[i][1];
          y2 = arr[j][1];
          y3 = arr[k][1];
          y4 = arr[h][1];
          if((x4-x1 !=0 || y4-y1 !=0) && (x1-x2 !=0 || y1-y2 !=0) && (x2-x3 !=0 || y2-y3 !=0) && (x3-x4 !=0 || y3-y4 !=0) && (x2-x4 !=0 || y2-y4 !=0)&& (x1-x3 !=0 || y1-y3 !=0)){
            if((x1 == x2 || y1 == y2) && (x2 == x3 || y2 == y3) && (x3 == x4 || y3 == y4) && (x4 == x1 || y4 == y1)){
              cout<<x1<<" "<<y1<<"  "<<x2<<" "<<y2<<"  "<<x3<<" "<<y3<<"  "<<x4<<" "<<y4<<endl;
              for(int r = 0; r < 12; r++){
                for(int u = 0; u < 4; u++){
                  if(u == 0){
                    dx1 = arr[h][0] - arr[i][0];
                    dy1 = arr[h][1] - arr[i][1];

                    dx = arr[r][0] - arr[i][0];
                    dy = arr[r][1] - arr[i][1];
                  } else if(u==1){
                    dx1 = arr[i][0] - arr[j][0];
                    dy1 = arr[i][1] - arr[j][1];

                    dx = arr[r][0] - arr[j][0];
                    dy = arr[r][1] - arr[j][1];
                  } else if(u==2){
                    dx1 = arr[j][0] - arr[k][0];
                    dy1 = arr[j][1] - arr[k][1];

                    dx = arr[r][0] - arr[k][0];
                    dy = arr[r][1] - arr[k][1];
                  } else {
                    dx1 = arr[k][0] - arr[h][0];
                    dy1 = arr[k][1] - arr[h][1];

                    dx = arr[r][0] - arr[h][0];
                    dy = arr[r][1] - arr[h][1];
                  }

                  limit = dx1 * dy - dx * dy1;
                  cout<<dx1<<" "<<dy<<" "<<dx<<" "<<dy1<<endl;
                  if(limit>0){
                    point++;
                  }
                }
                if(pointMax < point){
                    cout<<point<<" ytrtyr"<<endl;
                    pointMax = point;
                    point = 0;
                    pointArr[0][0] = arr[i][0];
                    pointArr[0][1] = arr[i][1];
                    pointArr[1][0] = arr[j][0];
                    pointArr[1][1] = arr[j][1]; 
                    pointArr[2][0] = arr[k][0];
                    pointArr[2][1] = arr[k][1];
                    pointArr[3][0] = arr[h][0];
                    pointArr[3][1] = arr[h][1];
                  } 
              }  
            }
          } 
        }
      }
    }
  }
  for(int t = 0; t < 4; t++){
    cout<<"Точка "<<t<<": "<<pointArr[t][0]<<" "<<pointArr[t][1]<<endl;
  }
  cout<<pointMax<<endl;
}
