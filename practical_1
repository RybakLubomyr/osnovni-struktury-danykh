#include <iostream>
#include <cmath>
using namespace std;
int main() {
  int arr[7][2];
  arr[0][0] = 4;
  arr[0][1] = 6;
  arr[1][0] = 3;
  arr[1][1] = 2;
  arr[2][0] = 8;
  arr[2][1] = 4;
  arr[3][0] = 3;
  arr[3][1] = 2;
  arr[4][0] = 9;
  arr[4][1] = 1;
  arr[5][0] = -3;
  arr[5][1] = 2;
  arr[6][0] = -7;
  arr[6][1] = 4;
  arr[6][0] = 1;
  arr[6][1] = 6;
  float d;
  float max=0;
  int number1,number2;
  for(int i =0; i < 7; i++){
    for(int j=i+1; j < 7; j++){
      if(i != j){
        d = sqrt((arr[j][0] - arr[i][0])*(arr[j][0] - arr[i][0]) + (arr[j][1] - arr[i][1])*(arr[j][1] - arr[i][1]));
        cout<<d<<endl;
      }
      if(max < d){
        max = d;
        number1 = i;
        number2 = j;
      }
      
    }
  }
  cout<<arr[number2][1]<<" "<< arr[number1][1]<<endl;
  float centerX=(arr[number2][0] + arr[number1][0])/2;
  float centerY=(arr[number2][1] + arr[number1][1])/2;
  cout<<"d: "<<max<<" Центр кола: x-"<<centerX<<" y-"<<centerY<<endl;
}
