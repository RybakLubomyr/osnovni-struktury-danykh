#include <iostream>
using namespace std;
int main() {
  int arr[10][2];
  int d[1][2];
  d[0][0] = 2;
  d[0][1] = 2;

  arr[0][0] = 3;
  arr[0][1] = 4;
  arr[1][0] = 5;
  arr[1][1] = 5;
  arr[2][0] = 11;
  arr[2][1] = 4;
  arr[3][0] = 7;
  arr[3][1] = 4;
  arr[4][0] = 3;
  arr[4][1] = 7;
  arr[5][0] = 7;
  arr[5][1] = 7;
  arr[6][0] = 6;
  arr[6][1] = 2;
  arr[7][0] = 8;
  arr[7][1] = 3;
  arr[8][0] = 9;
  arr[8][1] = 3;
  arr[9][0] = 8;
  arr[9][1] = 1;

  int sumParal = 0;
  int x1,x2,y1,y2;

  for(int i = 0; i < 10; i++){
    for(int j = 1; j < 10; j++){
      for(int k = 2; k < 10; k++){
        x1 = arr[k][0] - d[0][0];
        x2 = arr[j][0] - arr[i][0];
        y1 = arr[i][1] - d[0][1];
        y2 = arr[j][1] - arr[k][1];
        if(x1 == x2 && y1 == y2){
          sumParal++;
        }
      }
    }
  }
  cout<<sumParal<<endl;
}
